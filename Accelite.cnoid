toolbars: 
  "TimeBar": 
    minTime: 0
    maxTime: 15
    frameRate: 100
    playbackFrameRate: 100
    idleLoopDrivenMode: false
    currentTime: 0
    speedScale: 1
    syncToOngoingUpdates: false
  "BodyBar": 
    current: 3
    stanceWidth: 0.15
  "KinematicsBar": 
    mode: FK
    attitude: true
    penetrationBlock: true
    collisionLinkHighlight: true
    snapDistance: 0.025
    penetrationBlockDepth: 0.0001
    lazyCollisionDetectionMode: true
  "BodyMotionGenerationBar": 
    balancer: true
    autoGeneration: true
    timeScaleRatio: 1
    preInitialDuration: 1
    postFinalDuration: 1
