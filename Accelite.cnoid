               0.000000,  0.000000,  1.000000 ]
            zmp: [  0.000000,  0.000000,  0.000000 ]
            selfCollisionDetection: false
        - 
          id: 3
          name: "Accelite"
          plugin: Body
          class: BodyItem
          data: 
            modelFile: "Accelite.yaml"
            currentBaseLink: "Foot_R"
            rootPosition: [  0.005778, -0.006063,  0.303079 ]
            rootAttitude: [ 
               0.995031, -0.059854, -0.079568, 
               0.059242,  0.998193, -0.010041, 
               0.080026,  0.005277,  0.996779 ]
            jointPositions: [ 
               0.000000,  0.000000,  1.256637, -0.349066, -0.349066,  0.349066,  0.349066,  0.349066,  0.349066, 
               0.000000,  0.000000,  0.000000,  0.000000, -0.349066,  0.349066, -0.349066,  0.349066,  0.349066, 
              -0.349066,  0.000000,  0.000000, -0.261799,  0.000000,  0.261799,  0.000000 ]
            initialRootPosition: [  0.000000,  0.000000,  0.320000 ]
            initialRootAttitude: [ 
               1.000000,  0.000000,  0.000000, 
               0.000000,  1.000000,  0.000000, 
               0.000000,  0.000000,  1.000000 ]
            initialJointPositions: [ 
               0.000000,  0.000000,  0.000000,  0.000000,  0.000000,  0.000000,  0.000000,  0.000000,  0.000000, 
